@model List<_002.Collection.Models.Game>;

<!doctype html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width"/>
</head>
<body>
    @foreach (var game in Model)
    {
        <h2>Game: @game.Name</h2>
        <h2>Platform: @game.Platform</h2>
        <h2>Engine: @game.Engine</h2>
        <h2>@(new string('-', 30))</h2>
    }
</body>
</html>